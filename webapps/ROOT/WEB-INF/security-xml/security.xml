<?xml version="1.0" encoding="UTF-8"?>
<security xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../conf/security.xsd">
	<urls>
		<!-- Static Paths -->
		<url path="/static/[a-z]+/[a-z_]+\.[a-z]+" />
		<url path="/" method="get,post" />
		<url path="/login" method="get" authentication="required" />
		<url path="/verification" method="get" />
		<url path="/reset_password" method="get" />
		<url path="/signup" method="get" />
		<url path="/migration/runMigration" method="get" />
	</urls>
</security>