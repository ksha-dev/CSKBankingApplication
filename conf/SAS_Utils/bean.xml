<beans>
	<!-- Every HTTP operation brought in to one transation boundary  -->
    <Bean bean_name="CollaborationBean"
        bean_id="Bean:bean_id:CollaborationBean"
        module_id="SAS_Utils"
        transaction_type="1"
        classname="com.zoho.accounts.onprem.util.CollaborationBean">
    </Bean>
    <Bean bean_name="SlavePersistenceLite"
        bean_id="Bean:bean_id:SlavePersistenceLite"
        module_id="SAS_Utils"
        transaction_type="1"
        classname="com.adventnet.persistence.PersistenceBean">
    </Bean>
    
    <Bean bean_id="Bean:bean_id:SlaveDataSource" bean_name="SlaveDataSource" module_id="SAS_Utils" transaction_type="3" classname="com.zoho.accounts.onprem.util.SlaveDataSourceImpl">
        </Bean>
    
</beans>
